    "up": "taze latest -I"
  },
  "dependencies": {
    "@unocss/reset": "^0.45.22",
    "solid-js": "1.5.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.27.0",
    "eslint": "8.23.1",
    "lint-staged": "^13.0.3",
    "pnpm": "^7.12.1",
    "simple-git-hooks": "^2.8.0",
    "taze": "0.8.1",
    "typescript": "4.8.3",
    "unocss": "^0.45.22",
    "vite": "3.1.3",
    "vite-plugin-solid": "2.3.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,yaml,yml}": [
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": "@antfu",
    "rules": {
      "react/no-unknown-property": "off"
    }
  }
